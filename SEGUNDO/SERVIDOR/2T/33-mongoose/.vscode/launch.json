{
	"version": "0.2.0",
	"configurations": [
			{
					"type": "node",
					"request": "launch",
					"name": "Run Current File",
					"program": "${file}",
					"skipFiles": [
							"<node_internals>/**"
					]
			},
			{
					"type": "node",
					"request": "launch",
					"name": "Run src/index.js",
					"program": "${workspaceFolder}/src/index.js",
					"skipFiles": [
							"<node_internals>/**"
					]
			}
	]
}
